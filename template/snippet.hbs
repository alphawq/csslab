{{import "module/head"}}
{{import "module/header"}}
<div class="m-sidebar">
	<a class="m-sidebar-download" href="snippet.7z">
		Download
		<span>Sublime Snippet</span>
	</a>
	<h2 class="m-sidebar-title">Snippet v1.2 <span>(2017.1.22)</span></h2>
	<ul class="m-sidebar-list" id="m-sidebar-list">
		<h4>CSS</h4>
		<li class="current">fontface</li>
		<li>iconfont</li>
	</ul>
</div>
<div class="m-content" id="m-content-list">

<div class="m-content-list-item">
	<h3><b>fontface</b><i>自定义字体定义</i></h3>
	<pre><code data-language="css">
		@font-face{ 
		    font-family:'diyfont'; 
		    src: url('diyfont.eot');
		    src: url('diyfont.eot?#iefix') format('embedded-opentype'),
		    url('diyfont.woff') format('woff'),
		    url('diyfont.ttf') format('truetype'),
		    url('diyfont.svg#diyfont') format('svg');
		} 
	</code></pre>
</div>
<div class="m-content-list-item">
	<h3><b>iconfont</b><i>图标字体</i></h3>
	<pre><code data-language="css">
		font-family:"iconfont";
		font-style:normal;
		display:block;
		-webkit-font-smoothing: antialiased;
		-webkit-text-stroke-width: 0.2px;
	</code></pre>
</div>

</div>
